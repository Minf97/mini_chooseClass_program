.container {
  min-height: 100vh;
  position: relative;
}
.title-class {
  font-size: 35rpx;
  color: rgb(89, 128, 106);
  letter-spacing: 5rpx;
  font-weight: 500;
  margin: 25rpx 0;
}
.title-class-info {
  font-weight: 700;
  letter-spacing: 2rpx;
  margin-top: 25rpx;
}
.divider {
  margin: 0 !important;
}
.body {
  width: 100%;
  height: 100%;
  background-color: rgb(244, 249, 245);
}
.btn{
  position: absolute !important;
  bottom: 0;
  left: 50%;
  transform: translate(-50%,0);
  width: 90%;
  background-color: rgb(69, 129, 93) !important;
  border: 0 !important;
}